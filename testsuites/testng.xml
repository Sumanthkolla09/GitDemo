<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite"  parallel="tests" thread-count="5">
    <listeners>
        <listener class-name="TestComponents.Listeners"/>
    </listeners>
    <test name="SubmitOrder" >
        <classes>
            <class name="org.SeleniumTest.EntireE2E"/>
        </classes>
    </test>
    <test name="ErrorValidation" thread-count="5">
        <classes>
            <class name="org.SeleniumTest.ErrorValidation"/>
        </classes>
    </test>
</suite>